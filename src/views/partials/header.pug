if profileData != undefined
  if profileData.profile_banner_url != undefined
    header.circle--header(style={"background-image": `url("${profileData.profile_banner_url}")`})
      .bounds
        .circle--fluid
          .circle--fluid--cell
            a.logo(href='/')
              img(src=`${profileData.profile_image_url_https}`)
              h1 @#{profileData.screen_name}
          .circle--fluid--cell.align-right
            nav
              ul.circle--inline
                li
                  a(href='https://twitter.com/') Sign Out
  else
    header.circle--header
      .bounds
        .circle--fluid
          .circle--fluid--cell
            a.logo(href='/')
              img(src=`${profileData.profile_image_url_https}`)
              h1 @#{profileData.screen_name}
          .circle--fluid--cell.align-right
            nav
              ul.circle--inline
                li
                  a(href='https://twitter.com/') Sign Out
else
  header.circle--header
    .bounds
      .circle--fluid
        .circle--fluid--cell
          a.logo(href='/')
            include ../../public/images/twitter.svg
            case error[0].code
              when 186
                h1 @click_2_return
              when 404
                h1 @click_2_return
              default
                h1 @this_is_not_a_drill
        .circle--fluid--cell.align-right
          nav
            ul.circle--inline
              li
                a(href='https://twitter.com/') Sign Out
block content
